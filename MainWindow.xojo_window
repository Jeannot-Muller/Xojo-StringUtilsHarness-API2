	#tag Method, Flags = &h0, CompatibilityFlags = TargetHasGUI
		Sub Print(s As String)
		  if mSelChanged then
		    OutFld.SelectionStart = 999999
		  end if
		  
		  OutFld.SelectedText = s + EndOfLine
		  mSelChanged = false
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h1
		Protected mSelChanged As Boolean
	#tag EndProperty


#tag EndWindowCode

#tag Events OutFld
	#tag Event
		Sub SelChange()
		  mSelChanged = true
		  
		End Sub
	#tag EndEvent
